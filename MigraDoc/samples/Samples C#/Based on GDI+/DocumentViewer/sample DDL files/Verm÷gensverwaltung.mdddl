      Font.Name = "Verdana"
      Font.Size = 8
    }
    
    Heading1
    { 
      Font.Size = 36
      Font.Bold = true
    }
    
    Heading2
    {
      Font.Size = 20
    }
    
    Heading3
    {
      Font.Size = 18
      Paragraphformat
      {
        SpaceAfter = 12
        Tabstops += "3cm"
      }  
    }

    Heading4
    {
      Font.Size = 12
    }

    // header for portrait
    Header
    {
      ParagraphFormat
      {
        Font.Size = 7
        Font.Bold = true
        Borders.Bottom.Visible = true
        TabStops = null
        TabStops +=
        {
          Position = "8cm"
          Alignment = Center
        }
        TabStops +=
        {
          Position = "16cm"
          Alignment = Right
        }
      }
    }

    // header for landscape
    HeaderLandscape : Header
    {
      ParagraphFormat
      {
        TabStops = null
        TabStops +=
        {
          Position = "13cm"
          Alignment = Center
        }
        TabStops +=
        {
          Position = "26cm"
          Alignment = Right
        }
      }  
    }
        
    Footer : Header
    {
      ParagraphFormat
      {
        Font.bold = false
        Borders.Bottom.Visible = false
      }
    }

    InhaltFormatvorlage : Normal
    {
      Paragraphformat
      {
        LeftIndent = "1cm"
        Tabstops += 
        {
          Position = "16cm"
          Alignment = right
          Leader = Dots
        }
      }  
    }
    
    HyperLink
    {
      Font.Underline = None
      Font.Color = Navy
    }
    
    Note : DefaultParagraphFont
    {
      Font.Superscript = true
    }
    
    HeaderInfo1 : Header
    {
      Font.Size = 10
    }

    HeaderInfo2 : Header
    {
      Font.Size = 8
      ParagraphFormat
      {
        TabStops +=
        {
          Position  = "2.5cm"
          Alignment = Left
        }
      }
    }
    
    //WebInfo
    WebInfo : Normal
    {
      ParagraphFormat
      {
        Font
        {
          Name = "Verdana"
          Size = 8
          Color = 0xFF111111
        }
        LeftIndent = 14.2
        RightIndent = 14.2
        SpaceBefore = 14.2
        SpaceAfter = 14.2
        Borders
        {
          Visible = true
          Style = Single
          Width = 0.5
          Color = Gray
          DistanceFromTop = 7
          DistanceFromBottom = 7
          DistanceFromLeft = 7
          DistanceFromRight = 7
        }
        Shading
        {
          Color = 0xFFFFFFCC
        }
      }
    }
  }      

